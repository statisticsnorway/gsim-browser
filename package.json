{
  "name": "gsim-browser",
  "version": "0.2.0",
  "description": "A React application for browsing GSIM objects stored in LDS",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/statisticsnorway/dc-gsim-browser"
  },
  "dependencies": {
    "moment": "^2.23.0",
    "react": "^16.6.3",
    "react-components-library": "https://github.com/statisticsnorway/react-components-library.git",
    "react-datepicker": "^1.8.0",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-table": "^6.8.6",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.84.0"
  },
  "scripts": {
    "start" : "chmod +x ./docker/env.bash && ./docker/env.bash && cp ./env-config.js ./public/ && react-scripts start",
    "build": "sh -ac '. ./.env; react-scripts build'",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage --collectCoverageFrom=**/src/** --collectCoverageFrom=!**/src/index.js --collectCoverageFrom=!**/src/utilities/Fetch.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "react-test-renderer": "^16.6.3"
  }
}
