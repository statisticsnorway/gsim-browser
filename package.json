{
  "name": "gsim-browser",
  "version": "0.2.0",
  "description": "A React application for browsing GSIM objects stored in LDS",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/statisticsnorway/dc-gsim-browser"
  },
  "dependencies": {
    "moment": "^2.23.0",
    "react": "^16.6.3",
    "react-components-library": "https://github.com/statisticsnorway/react-components-library.git",
    "react-datepicker": "1.8.0",
    "react-dom": "^16.6.3",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.8",
    "react-table": "^6.8.6",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.87.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose=false",
    "coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/index.js",
      "!src/utilities/Enum.js",
      "!src/utilities/Fetch.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
